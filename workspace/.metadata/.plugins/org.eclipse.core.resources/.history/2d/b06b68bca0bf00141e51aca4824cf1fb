
public class MusicController {

	public static void main(String[] args){
		
		MusicPlayer mp = new MusicPlayer();
		
		for (int i = 0; i < s.length(); i++) {

			char c = s.charAt(i);

			switch(c){
				case 'A' :
					playNote('C',4);
					break;
				case 'G' :
					playNote('D',4);
					break;
				case 'C' :
					playNote('E',4);
					break;
				case 'T' :
					playNote('F',4);
					break;
			}

		}
	}
	
}
